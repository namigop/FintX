<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:typeEditors="clr-namespace:Tefin.ViewModels.Types.TypeEditors"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="typeEditors:BoolEditor"
             x:Class="Tefin.Views.Types.TypeEditors.BoolEditorView">
    <Border x:Name="MainBorder">
        <StackPanel Orientation="Horizontal">
            <ToggleSwitch IsChecked="{Binding TempValue, Mode=TwoWay}"
                          OffContent="False"
                          OnContent="True" />

        </StackPanel>

    </Border>
</UserControl>